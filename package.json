{
  "name": "opinionated-redux",
  "version": "0.0.1",
  "description": "an opinionated redux setup with redux-observable, redux-devtools and redux-persist",
  "keywords": [
    "opinionated-redux",
    "redux",
    "observable",
    "rxjs",
    "devtools",
    "persist"
  ],
  "homepage": "https://github.com/silicon-hills/opinionated-redux",
  "bugs": {
    "url": "https://github.com/silicon-hills/opinionated-redux/issues",
    "email": "hello@nuevesolutions.com"
  },
  "license": "Apache-2.0",
  "author": {
    "name": "Silicon Hills LLC",
    "email": "hello@nuevesolutions.com",
    "url": "https://nuevesolutions.com"
  },
  "contributors": [
    "Jam Risser <jam@codejam.ninja> (https://codejam.ninja)"
  ],
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/silicon-hills/opinionated-redux"
  },
  "scripts": {
    "build": "make -s build",
    "clean": "make -s clean",
    "coverage": "make -s coverage",
    "format": "make -s format",
    "inc": "npm version patch --git=false 2>/dev/null || true",
    "lint": "make -s lint",
    "make": "make",
    "prepare": "make -s prepare",
    "purge": "make -s purge",
    "report": "make -s report",
    "spellcheck": "make -s spellcheck",
    "start": "make -s start",
    "test": "make -s test",
    "test:ui": "make test-ui",
    "test:watch": "make -s test-watch"
  },
  "dependencies": {
    "@babel/runtime": "^7.11.2",
    "core-js": "^3.6.5",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-observable": "^1.2.0",
    "redux-persist": "^6.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.4",
    "@babel/node": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-decorators": "^7.10.5",
    "@babel/plugin-proposal-optional-chaining": "^7.11.0",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-typescript": "^7.10.4",
    "@types/jest": "^26.0.10",
    "@types/react": "^16.9.52",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "cspell": "^4.1.0",
    "eslint": "^7.7.0",
    "eslint-config-airbnb-typescript": "^9.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "jest": "^26.4.2",
    "jest-sonar-reporter": "^2.0.0",
    "lockfile-lint": "^4.3.7",
    "majestic": "^1.7.0",
    "prettier": "^2.1.1",
    "typescript": "^4.0.2"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "eslintIgnore": [
    "lib"
  ],
  "jest": {
    "testRegex": "tests(/|/.*/)[^_/]*[jt]sx?$",
    "verbose": true
  },
  "jestSonar": {
    "reportPath": "node_modules/.tmp/reports",
    "reportFile": "test-report.xml",
    "indent": 4
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "none"
  }
}
